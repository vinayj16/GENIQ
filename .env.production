# Production Environment Variables
VITE_API_URL=/api
NODE_ENV=production
VITE_API_KEY=prod_secure_key_${RANDOM_UUID}

# Backend URL (will be set during deployment)
VITE_BACKEND_URL=${BACKEND_URL}

# Google AI API Key (should be set in deployment environment)
VITE_GOOGLE_AI_API_KEY=${GOOGLE_AI_API_KEY}

# JWT Configuration
JWT_SECRET=${JWT_SECRET}
JWT_EXPIRES_IN=30d

# CORS Configuration
FRONTEND_URL=${FRONTEND_URL}

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX=100
