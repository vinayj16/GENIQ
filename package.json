{
  "name": "geniq-interview-prep",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "build:prod": "vite build --mode production --emptyOutDir",
    "build:render": "npm install && npm run build:prod && cd backend && npm install",
    "build:dev": "vite build --mode development",
    "lint": "npm run lint:js && npm run lint:css && npm run check:css-vars && npm run check:css-classes",
    "preview": "vite preview",
    "test-api": "node test-api.js",
    "start": "node backend/start-prod.js",
    "start:prod": "NODE_ENV=production node backend/start-prod.js",
    "start-backend": "cd backend && npm start",
    "start-full": "concurrently \"npm run start-backend\" \"npm run dev\"",
    "setup": "node scripts/setup-env.js",
    "deploy:build": "npm run build:prod",
    "verify-build": "node scripts/verify-build.js",
    "check-env": "node -e \"console.log('API URL:', process.env.VITE_API_URL || 'Not set'); console.log('API Key:', process.env.VITE_API_KEY ? '****' + process.env.VITE_API_KEY.slice(-4) : 'Not set');\"",
    "heroku-postbuild": "npm run build",
    "render-build": "npm run build:render",
    "lint:css": "stylelint \"src/**/*.css\" --fix --quiet",
    "lint:js": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --quiet",
    "check:css-vars": "node scripts/check-css-variables.js",
    "check:css-classes": "node scripts/check-css-classes.js"
  },
